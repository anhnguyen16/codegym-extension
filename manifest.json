{
    "manifest_version": 3,
    "name": "CodeGym Extension",
    "version": "1.0",
    "description": "Customize your web app appearance.",
    "permissions": ["storage", "activeTab"],
    "content_scripts": [
      {
        "matches": ["*://*.codegym.vn/*"],
        "js": ["content.js"]
      }
    ]
  }
  